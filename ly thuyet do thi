//hàm void init_graph(Graph *pG, int n) 
void init_graph(Graph *pG, int n){
    pG->n = n;
    pG->m = 0;
}

//hàm add_edge(Graph *pG, int u, int v) để thêm cung (u, v) vào đồ thị pG theo mẫu (prototype):
void add_edge(Graph *pG, int u, int v) {
    pG->edges[pG->m].u=u;
    pG->edges[pG->m].v=v;
    pG->m++;
}
